{
  "scripts": {
    "bootstrap": "bundle install && npm install",
    "clean": "rm -rf dist && mkdir dist",
    "build": "npm run clean && npm run static && npm run sass",
    "static": "rsync -a src/** dist --exclude sass",
    "sass": "bundle exec compass compile",
    "open": "open dist/index.html",
    "deploy": "gh-pages --dist dist --branch master"
  },
  "devDependencies": {
    "gh-pages": "^2.1.1"
  }
}
